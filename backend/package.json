{
  "name": "gamification_backend",
  "version": "1.0.0",
  "scripts": {
    "start:dev": "nodemon",
    "start": " node ./dist",
    "build": "tsc --build",
    "commit-a": "git add . && git cz",
    "commit": "git cz",
    "lint": "tslint --project \"tsconfig.json\"",
    "test": "jest --testTimeout=10000",
    "generate-docs": "typedoc --tsconfig ./tsconfig.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.2.5",
    "sequelize": "^6.33.0",
    "snyk": "^1.729.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/helmet": "^4.0.0",
    "@types/jsonfile": "^6.0.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.17.20",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "find": "^0.3.0",
    "fs-extra": "^9.1.0",
    "git-cz": "^4.7.6",
    "jsonfile": "^6.1.0",
    "nodemon": "^2.0.13",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.11.0",
    "typedoc": "^0.20.37",
    "typescript": "^5.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
